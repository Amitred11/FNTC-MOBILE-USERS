{
  "cli": {
    "version": ">= 16.15.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "API_URL": "${secret.API_URL}",
        "WEB_CLIENT_ID": "${secret.WEB_CLIENT_ID}",
        "IOS_CLIENT_ID": "${secret.IOS_CLIENT_ID}",
        "ANDROID_CLIENT_ID": "${secret.ANDROID_CLIENT_ID}",
        "CONFIG_INTERNAL_API_KEY": "${secret.CONFIG_INTERNAL_API_KEY}"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      },
      "env": {
        "API_URL": "${secret.API_URL}",
        "WEB_CLIENT_ID": "${secret.WEB_CLIENT_ID}",
        "IOS_CLIENT_ID": "${secret.IOS_CLIENT_ID}",
        "ANDROID_CLIENT_ID": "${secret.ANDROID_CLIENT_ID}",
        "CONFIG_INTERNAL_API_KEY": "${secret.CONFIG_INTERNAL_API_KEY}",
        "CHATBOT_API_ENDPOINT": "${secret.CHATBOT_API_ENDPOINT}"
      }
    }
  },
  "submit": {
    "production": {}
  }
}