{
  "cli": {
    "version": ">= 16.15.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      },
      "env": {
        "API_URL": "@API_URL",
        "INTERNAL_API_KEY": "@INTERNAL_API_KEY",
        "CHATBOT_API_ENDPOINT": "@CHATBOT_API_ENDPOINT"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
